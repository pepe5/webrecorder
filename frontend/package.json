{
  "name": "webrecorder-frontend",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "optionalDependencies": {
    "fsevents": "*"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "eslint": "3.6.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "7.5.1",
    "jest": "15.1.1",
    "nodemon": "^1.15.0",
    "npm-delay": "^1.0.4",
    "npm-run-all": "^4.1.2",
    "redux-devtools": "^3.4.0",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "@babel/core": "7.0.0-beta.37",
    "autoprefixer": "6.4.0",
    "babel-eslint": "6.1.2",
    "babel-jest": "15.0.0",
    "babel-loader": "7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-plugin-typecheck": "^3.6.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "better-npm-run": "^0.0.11",
    "bootstrap-loader": "^2.2.0",
    "bootstrap-sass": "^3.3.7",
    "circular-dependency-plugin": "^4.4.0",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.6",
    "compression": "^1.6.0",
    "css-loader": "0.24.0",
    "express": "^4.16.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^0.9.0",
    "filesize": "3.3.0",
    "gzip-size": "3.0.0",
    "hard-source-webpack-plugin": "^0.5.16",
    "http-proxy-middleware": "0.17.1",
    "immutable": "^3.8.1",
    "node-sass": "^4.3.0",
    "object-assign": "4.1.0",
    "piping": "^1.0.0-rc.4",
    "postcss-loader": "0.11.1",
    "prop-types": "^15.5.8",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-collapsible": "github:m4rk3r/react-collapsible#dist",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-markdown": "^3.1.1",
    "react-redux": "^5.0.5",
    "react-router-breadcrumbs-hoc": "github:m4rk3r/react-router-breadcrumbs-hoc#dist",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-toggle": "^4.0.2",
    "react-virtualized": "^9.11.1",
    "redbox-react": "^1.3.3",
    "redux": "^3.7.1",
    "redux-connect": "^7.0.0",
    "redux-immutable": "^4.0.0",
    "redux-search": "^2.4.0",
    "reselect": "^3.0.1",
    "resolve-url-loader": "^2.2.1",
    "sass-loader": "^4.0.2",
    "serialize-javascript": "^1.1.2",
    "strip-loader": "^0.1.0",
    "style-loader": "0.13.1",
    "superagent": "^3.3.1",
    "universal-webpack": "^0.6.0",
    "url-loader": "0.5.7",
    "webpack": "3.10.0",
    "webpack-dev-middleware": "^2.0.5",
    "webpack-hot-middleware": "^2.21.0",
    "whatwg-fetch": "1.0.0"
  },
  "engines": {
    "node": ">=6.9 <=8.9.1"
  },
  "scripts": {
    "analyze": "NODE_ENV=\"production\" webpack --config ./webpack/webpack.config.client.production.babel.js --json | webpack-bundle-size-analyzer",
    "stats": "NODE_ENV=\"production\" webpack --config ./webpack/webpack.config.client.production.babel.js --json --profile > stats.json",
    "delay": "node ./node_modules/npm-delay 15000",
    "dev": "run-p dev:client:build dev:server:prepare dev:server:build dev:server:delay",
    "dev:client:build": "better-npm-run build-client-local",
    "dev:server:prepare": "universal-webpack --settings ./webpack/universal-webpack-settings.json prepare",
    "dev:server:build": "better-npm-run dev:server:build-local",
    "dev:server:delay": "run-s delay dev:server:start",
    "dev:server:start": "better-npm-run start-dev",
    "build-client-local": "better-npm-run build-client-local",
    "docker-dev": "run-p docker-dev:client:build dev:server:prepare docker-dev:server:build docker-dev:server:delay",
    "docker-dev:client:build": "better-npm-run build-client",
    "docker-dev:server:build": "better-npm-run dev:server:build",
    "docker-dev:server:delay": "run-s delay docker-dev:server:start",
    "docker-dev:server:start": "better-npm-run start-docker-dev",
    "build-client": "better-npm-run build-client",
    "start-docker-dev": "better-npm-run start-docker-dev",
    "docker-prod": "run-s prod:client:build prod:server:build start-docker-prod",
    "prod:client:build": "webpack --config ./webpack/webpack.config.client.production.babel.js --colors --display-error-details",
    "prod:server:build": "webpack --config ./webpack/webpack.config.server.production.babel.js --colors --display-error-details",
    "start-docker-prod": "better-npm-run start-docker-prod",
    "test": "jest --watch --env=jsdom"
  },
  "betterScripts": {
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config ./webpack/webpack.config.client.production.babel.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "dev:server:build-local": {
      "command": "webpack --config ./webpack/webpack.config.server.development.babel.js --watch --colors --display-error-details",
      "env": {
        "NODE_ENV": "development",
        "FRONTEND_PORT": 8095,
        "APP_HOST": "localhost:8095",
        "CONTENT_HOST": "localhost:8092",
        "INTERNAL_HOST": "localhost",
        "INTERNAL_PORT": 8089
      }
    },
    "dev:server:build": {
      "command": "webpack --config ./webpack/webpack.config.server.development.babel.js --watch --colors --display-error-details",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start-dev": {
      "command": "nodemon ./bin/server.babel.js --watch ./static",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "FRONTEND_PORT": 8095,
        "CONTENT_HOST": "localhost:8092",
        "INTERNAL_HOST": "localhost",
        "INTERNAL_PORT": 8089
      }
    },
    "start-docker-dev": {
      "command": "nodemon ./bin/server.babel.js --watch ./static",
      "env": {
        "NODE_PATH": "./src"
      }
    },
    "start-docker-prod": {
      "command": "node ./bin/server.babel.js",
      "env": {
        "NODE_PATH": "./src"
      }
    },
    "build-client-local": {
      "command": "babel-node webpack/webpack-dev-server.js",
      "env": {
        "NODE_ENV": "development",
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "FRONTEND_PORT": 8095,
        "CONTENT_HOST": "localhost:8092"
      }
    },
    "build-client": {
      "command": "babel-node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src"
      }
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|node_modules|static)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "moduleNameMapper": {
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/config/jest/fileTransform.js",
      ".+\\.(css|scss)$": "<rootDir>/config/jest/cssTransform.js"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  }
}
